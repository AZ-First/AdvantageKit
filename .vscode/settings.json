{
    "files.exclude": {
        "external": true,
        "bazel-*": false,
        "compile_commands.json": false,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.project.referencedLibraries": [
        "bazel-bin/external/maven/**/*.jar",
        "bazel-bin/external/frcmaven/**/*.jar",
        "bazel-bin/third_party/wpilib/downloaded/wpilibj.jar",
    ],
    "java.project.sourcePaths": [
        "build_tools/jpatch/sigcheck",
        "example_java_project/src",
        "junction/core/src",
        "junction/shims/wpilib/src",
        "conduit/api/src",
        "conduit/wpilibio/src/java"
    ],
    "bsv.cc.compdb.targets": [
        "//conduit/wpilibio"
    ],
    "files.associations": {
        "BUILD": "bazel",
        "WORKSPACE": "bazel",
    }
}